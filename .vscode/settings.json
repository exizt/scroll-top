{
    "cSpell.words": [
        "scrolltop"
    ],
    "search.exclude": {
        "**/.github": true,
        "**/*.code-workspace": true,
        "**/LICENSE": true,
        "**/package-lock.json": true
    },
    "liveSassCompile.settings.forceBaseDirectory": "/src/scss/*.scss",
    "liveSassCompile.settings.formats":[
        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": "~/../../dist",
            "savePathReplacementPairs": null
        },
    ],
    "liveSassCompile.settings.generateMap": false,
}